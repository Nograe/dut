#include <stdio.h>

void move(int tab[], int taille, int a, int b){

  int i;
  int temp=0;

  if(a>b && b>=taille){
    return;
  }

  temp=tab[a];
  tab[a]=tab[b];

  for(i=a+1; i<=b-1; i++){
    tab[i]=temp;
    temp=
  }

}

void main(){
  int i;
  int tab[9]={1,2,3,4,5,6,7,8};
  move(tab,9,2,6);
  
  for(i=0; i<9; i++){
    printf("i: %d\n", tab[i]);
  }


}
